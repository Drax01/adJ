+	<-N>;<-M>;<.9>;<:9>;</->;</=>;<.3>;<%0>;<<1>;</>1>;\
+	<N0>;<TM>;<sb>;<FB>;<DT>;\
+#
+	<D%>;<G%>;<g%>;\
+	<LJ>;<NJ>;<KJ>;<Ts>;<DZ>;\
+	<d%>;\
+	<lj>;<nj>;<kj>;<ts>;<dz>;\
+	<V%>;<v%>;<J%>;<G3>;\
+	<IO>;<IE>;<YI>;\
+	<II>;<ii>;<g3>;\
+	<io>;<ie>;<j%>;<DS>;<ds>;<yi>
