diff -ruN -x obj -x CVS -x *~ -x *patch -x *orig src54-s/share/locale/colldef/Makefile src/share/locale/colldef/Makefile
--- src54-s/share/locale/colldef/Makefile	Wed Dec 31 19:00:00 1969
+++ src/share/locale/colldef/Makefile	Sat Oct 12 18:32:19 2013
@@ -0,0 +1,114 @@
+.include <bsd.own.mk>
+
+
+LOCALES = ISO8859-1
+LOCALES += ISO8859-15
+
+LOCALES += de.ISO8859-1
+LOCALES += de.ISO8859-15
+LOCALES_UTF8 += de
+
+LOCALES += es.ISO8859-1
+LOCALES += es.ISO8859-15
+LOCALES_UTF8 += es
+
+LOCALES += is.ISO8859-1
+LOCALES += is.ISO8859-15
+LOCALES_UTF8 += is
+
+LOCALES += no.ISO8859-1
+LOCALES += no.ISO8859-15
+LOCALES_UTF8 += no
+
+LOCALES += sv.ISO8859-1
+LOCALES += sv.ISO8859-15
+LOCALES_UTF8 += sv
+
+LOCALES += ARMSCII-8
+LOCALES += CP1251
+LOCALES += CP866
+LOCALES += ISO8859-4
+LOCALES += ISO8859-5
+LOCALES += ISO8859-7
+LOCALES += ISO8859-13
+LOCALES += KOI8-R
+LOCALES += KOI8-U
+LOCALES += ISO8859-2
+LOCALES += pl.ISO8859-2
+LOCALES += sl.ISO8859-2 
+
+LFILES += ${LOCALES:S/$/.out/g} UTF-8.out ${LOCALES_UTF8:S/$/.UTF-8.out/g}
+
+all: ${LFILES}
+
+realall: ${LFILES}
+
+.SUFFIXES: .src .out
+
+.src.out:
+	colldef -I ${.CURDIR} -o ${.TARGET} ${.IMPSRC}
+
+NOMAN=	# defined
+
+.for c in ${LOCALES}
+${c}.out: ${c}.src
+	colldef -I ${.CURDIR} -o ${c}.out ${.CURDIR}/${c}.src
+.endfor
+
+UTF-8.out: ISO8859-1.src
+	colldef -I ${.CURDIR} -o UTF-8.out ${.CURDIR}/ISO8859-1.src
+
+.for c in ${LOCALES_UTF8}
+${c}.UTF-8.out: ${c}.ISO8859-1.src
+	colldef -I ${.CURDIR} -o ${c}.UTF-8.out ${.CURDIR}/${c}.ISO8859-1.src
+.endfor
+
+
+CLEANFILES += ${LFILES}
+
+
+install:
+.for locale in ${LOCALES}
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${DIRMODE} -d \
+	    ${DESTDIR}${LOCALEDIR}/${locale}
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${LOCALEMODE} \
+	    ${locale}.out ${DESTDIR}${LOCALEDIR}/${locale}/LC_COLLATE
+.endfor
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${DIRMODE} -d \
+	    ${DESTDIR}${LOCALEDIR}/UTF-8
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${LOCALEMODE} \
+	    UTF-8.out ${DESTDIR}${LOCALEDIR}/UTF-8/LC_COLLATE
+.for locale in ${LOCALES_UTF8}
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${DIRMODE} -d \
+	    ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/
+	install -o ${LOCALEOWN} -g ${LOCALEGRP} -m ${LOCALEMODE} \
+	    ${locale}.UTF-8.out ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/LC_COLLATE
+.endfor
+
+uninstall:
+.for locale in ${LOCALES}
+	if (test -f ${DESTDIR}${LOCALEDIR}/${locale}/LC_COLLATE) then { \
+	rm -f ${DESTDIR}${LOCALEDIR}/${locale}/LC_COLLATE; \
+	} fi; \
+        if (test -d ${DESTDIR}${LOCALEDIR}/${locale}/) then { \
+        r=`ls ${DESTDIR}${LOCALEDIR}/${locale}/`; \
+        if (test "$$r" = "") then { \
+        rm -rf ${DESTDIR}${LOCALEDIR}/${locale}/; \
+        } fi; \
+        } fi;
+.endfor
+	rm -f ${DESTDIR}${LOCALEDIR}/UTF-8/LC_COLLATE
+.for locale in ${LOCALES_UTF8}
+	if (test -f ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/LC_COLLATE) then { \
+	rm -f ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/LC_COLLATE; \
+	} fi; \
+        if (test -d ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/) then { \
+        r=`ls ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/`; \
+        if (test "$$r" = "") then { \
+        rm -rf ${DESTDIR}${LOCALEDIR}/${locale}.UTF-8/; \
+        } fi; \
+        } fi;
+.endfor
+
+
+.include <bsd.prog.mk>
diff -ruN -x obj -x CVS -x *~ -x *patch -x *orig src54-s/share/locale/colldef/ARMSCII-8.src src/share/locale/colldef/ARMSCII-8.src
--- src54-s/share/locale/colldef/ARMSCII-8.src	Wed Dec 31 19:00:00 1969
+++ src/share/locale/colldef/ARMSCII-8.src	Sat Oct 12 13:23:41 2013
@@ -0,0 +1,31 @@
+# $FreeBSD$
+#
+# LC_COLLATE source for Armenian/ARMSCII-8 (backward compatible with ASCII)
+# Made available by Vahe Khachikyan <vahe@khachikyan.de>
+#
+charmap map.ARMSCII-8
+substitute <armew> with "??"
+order \
+# controls
+	<armeternity>;<NU>;...;<US>;\x80;...;\xA0;\
+#
+	<SP>;(!,<armexclam>,<armaccent>);(\",<armquotleft>,<armquotright>);<Nb>;\
+	<DO>;<armemdash>;\
+	%;&;(',<armapostrophe>);(\(,<armparenleft>);(\),<armparenright>);*;+;\
+	(\,,<armcomma>);(-,<armendash>);<armyentamna>;<armellipsis>;(.,<armfullstop>);/;\
+# digits
+	0;...;9;\
+#
+	(:,<armdot>);<armsep>;\;;\<;=;>;(?,<armquestion>);<At>;\
+# capital
+	A;...;Z;\
+#
+	[;\\;];^;_;`;\
+# small
+	a;...;z;\
+# remaining punktuation etc
+	\{;|;\};~;<DT>;\
+# Armenian
+	<Armayb>;...;<armfe>;\
+# and the rest
+	\xff
